    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "moment": "^2.29.1",
    "vant": "^2.12.27",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vue-server-renderer": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "babel-loader": "^8.2.2",
    "cross-env": "^5.0.5",
    "css-loader": "^6.2.0",
